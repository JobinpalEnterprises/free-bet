<div id="group-{{ group }}" class="group-match-list">
    <h2>{{ 'soccer_group_name'|trans({'%group%': group}, "soccer")  }}</h2>
    <ul>
        {% for event in events %}
        <li>{{ event.date|date('Y-m-d H:i:s') }} : {{ event.leftName }} - {{ event.rightName }}</li>
        {% endfor %}
    </ul>
</div>